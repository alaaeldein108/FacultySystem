@model DepartmentViewModel
@{
    ViewData["Title"] = "Details";
}
<h1>Department Details</h1>

<div>
    <div class="row">
        <div class="col-12">
            <div class="form-group">
                <label asp-for="@Model.Code" class="col-form-label"></label>
                <input asp-for="@Model.Code" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Name" class="col-form-label"></label>
                <input asp-for="@Model.Name" class="form-control"readonly/>
            </div>
            <div class="form-group">
                <label asp-for="@Model.CarateAt" class="col-form-label"></label>
                <input asp-for="@Model.CarateAt" class="form-control" readonly />
            </div>
        </div>
    </div>
    </br>
    </br>
    <a class="btn btn-success" asp-action="Update" asp-route-id="@Model.Id">Update</a>
    <a asp-controller="Department" asp-action="Index" class="btn btn-light">Back To List</a>
</div>
