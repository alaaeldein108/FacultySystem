@model ApplicationRole
@{
    ViewData["Title"] = "Details";
}
<h1>Role Details</h1>

<div>
    <div class="row">
        <div class="col-12">
            <div class="form-group">
                <label asp-for="@Model.Name" class="col-form-label"></label>
                <input asp-for="@Model.Name" class="form-control" readonly />
            </div>
            <div class="form-group">
                <label asp-for="@Model.CreateAt" class="col-form-label"></label>
                <input asp-for="@Model.CreateAt" class="form-control" readonly />
            </div>
        </div>
    </div>  
    <br/>
    <a asp-controller="Role" class="btn btn-success" asp-action="Update" asp-route-id="@Model.Id">Update</a>
    <a asp-controller="Role" asp-action="Index" class="btn btn-light">Back To List</a>
</div>

